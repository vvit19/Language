энергос main ()
{
    пацан n = 0;
    внутрь (n);

    пацан res = позови fib (n);
    наружу (res);

    жрать 0;
}

энергос fib (n)
{
    если (n == 1)
    {
        жрать 1;
    }

    если (n == 2)
    {
        жрать 1;
    }

    пацан f_1 = 1;
    пацан f_2 = 1;
    пацан fib = 0;

    пацан i = 0;
    пока (i < n - 2)
    {
        fib = f_1 + f_2;
        f_1 = f_2;
        f_2 = fib;
        i = i + 1;
    }

    жрать fib;
}
